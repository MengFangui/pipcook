{
  "name": "@pipcook/daemon",
  "version": "1.0.0",
  "description": "pipcook daemon",
  "private": true,
  "dependencies": {
    "@types/cls-hooked": "^4.3.0",
    "cls-hooked": "^4.2.2",
    "egg-scripts": "^2.10.0",
    "midway": "^1.0.0",
    "sequelize": "^5.21.7",
    "sqlite3": "^4.1.1",
    "uuid": "^8.0.0",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.5.5",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.8.0",
    "midway-bin": "1",
    "midway-mock": "1",
    "tslib": "^1.8.1",
    "tslint-midway-contrib": "1",
    "ts-node": "^8.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.5.0",
    "@pipcook/pipcook-core": "^0.5.16",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "fs-extra": "^8.1.0",
    "@tensorflow/tfjs-node-gpu": "1.7.0",
    "jimp": "^0.10.0",
    "axios": "^0.18.1",
    "chalk": "^3.0.0",
    "rxjs": "^6.5.3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=midway-server-my_first_midway_project --framework=midway --ts",
    "stop": "egg-scripts stop --title=midway-server-my_first_midway_project",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "clear": "midway-bin clean",
    "dev": "ts-node init.ts && cross-env NODE_ENV=local midway-bin dev --port=6927 --ts",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint --fix -p tsconfig.json -t stylish",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "clean": "rm -rf src/app/public"
  },
  "ci": {
    "version": "10"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "anonymous",
  "license": "MIT"
}
